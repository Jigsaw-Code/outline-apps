{
  "compilerOptions": {
    "target": "es5",
    "downlevelIteration": true,
    "removeComments": false,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../../www",
    "lib": [
      "dom",
      "es5",
      "es2015.promise",
      "es2015.iterable",
      "es2015.collection",
      "es2016",
      "es2017",
      "scripthost"
    ]
  },
  "include": [
    "**/*.ts"
  ]
}
